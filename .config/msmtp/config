
account rmmichael95@gmail.com
host smtp.gmail.com
port 587
from rmmichael95@gmail.com
user rmmichael95@gmail.com
passwordeval "pass rmmichael95@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/ryanm/.config/msmtp/msmtp.log
# tls_starttls

