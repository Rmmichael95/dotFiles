;   ;; 番号０番、タイプ＝画像、画像＝パーツ画像０番、幅,高さ(200, 150)
;   (0
;    (type . image) 
;    (size . (200 150))
;    (url . 0))
;   ;; 画像はsizeに引き伸ばされます


;   ;; 番号１番、タイプ＝画像、画像＝パーツ画像２番＆矩形「(x,y,w,h)=(10,20,400,300)」
;   (1
;    (type . image) 
;    (size . (400 300))
;    (url  . 2)
;    (rect . (10 20 400 300)))
;   ;; 画像の10,20〜409,319までが使われます。

;   ;; 番号２番、タイプ＝画像、画像＝パーツ画像10番
;   (2
;    (type . image) 
;    (url  . 10))
;   ;; サイズ指定がない場合は画像のサイズになります

;   ;; 番号３番、タイプ＝画像、画像＝パーツ画像10番
;   (3
;    (type . image) 
;    (url  . 10)
;    (rect . (10 20 400 300)))
;   ;; サイズ指定がないが、rect指定がある場合はrectのw,hがサイズになります

;   ;; 番号４番、タイプ＝画像、画像＝パーツ画像10番
;   (4
;    (type . image) 
;    (url . "hoge.png"))
;   ;; urlに文字列を指定するとファイル名となります

;   ;; 番号１０番、タイプ＝ボタン、画像４枚
;   (5
;    (type . button) 
;    (size . (32 32))
;    ;; 画像４枚
;    (pic0 . ((url . 33)))
;    (pic1 . ((url . 34)))
;    (pic2 . ((url . 35)))
;    (pic3 . ((url . 36)))
;    )


;   ;; 番号１１番、タイプ＝ボタン、画像４枚
;   (11
;    (type . button) 
;    (pic0 . ((url . 37) (rect . ( 0  0 32 32))))
;    (pic1 . ((url . 37) (rect . (32  0 32 32))))
;    (pic2 . ((url . 37) (rect . ( 0 32 32 32))))
;    (pic3 . ((url . 37) (rect . (32 32 32 32))))
;    )

;   ;; このように同じ画像を使うことも可能
;   ;; サイズ指定がない場合は-0のが使われます

;   ;; 番号21番、タイプ＝アニメーション
;   (21
;    (type . animation) 
;    (size . (32 32))
;    (piclist . (
; 	       ((url . 100) (waittime . 100))
; 	       ((url . 101) (waittime . 200))
; 	       ((url . 102) (waittime . 300))
; 	       ((url . 103) (waittime . 200))
; 	       )
; 	    )
;    )


;   (22
;    (type . animation) 
;    (piclist . (
; 	       ((url . 100) (waittime . 200))
; 	       ((url . 101))
; 	       ((url . 102))
; 	       ((url . 103))
; 	       )
; 	    )
;    )
;   ;; 待ち時間は直前のものが引き継がれます
  
;   (23
;    (type . animation) 
;    (piclist . (
; 	       ((url . 37) (rect . ( 0  0 32 32)) (waittime . 200))
; 	       ((url . 37) (rect . (32  0 32 32)))
; 	       ((url . 37) (rect . ( 0 32 32 32)))
; 	       ((url . 37) (rect . (32 32 32 32)))
; 	       )
; 	    )
;    )
;   ;; rect指定も可能です

;   ;; キー待ちアニメーションはだいたいこうなります

;   ;; ファイル直接指定
;   (24
;    (type . animation) 
;    (size . (32 32))
;    (piclist . (
; 	       ((url . "IMG/0.png") (waittime . 1000))
; 	       ((url . "IMG/1.png"))
; 	       ((url . "IMG/2.png"))
; 	       ((url . "IMG/3.png"))
; 	       )
; 	    )
;    )




;   ;; 画像をひとつにしておくとよいでしょう
