;;#----------------------------------------------------------------------------
;;#メッセージボックス指定
;;#----------------------------------------------------------------------------
;;@MESSAGEBOX

(:message-window
  (entryMap 

  ;;※注意※
  ;;必ず 0 は必要

  ;;従来仕様（小フォント）
    (0

  ;;
  ;; Window settings
  ;;

  (window-position-x  . 40)
  (window-position-y  . 400)
  (window-width       . 720)
  (window-height      . 200)
  (window-bg-imageUrl . "parts/msgwin.png")  ;; 画像のurl

  ;;
  ;; Text Area Settings
  ;;

  (ta-offset-x .  10)  ;; ウィンドウからのオフセット
  (ta-offset-y .  32)  ;; ボタン24dot、残りは200-24=176, 32dot*4line=128
  (ta-width    . 700)
  (ta-height   . 176)

  ;; エリア背景
  (ta-bg-amikake . #f)     ;; #fで塗りつぶさない
  (ta-bg-color.r .  76)
  (ta-bg-color.g .  76)
  (ta-bg-color.b . 153)
  (ta-bg-color.a . 128)

  ;; フォント設定
  (ta-font       .  0)           ;; リソース定義にあるフォント番号を指定します
  (ta-font-size  . 22)           ;; 全角幅をピクセル指定です(偶数のみ)
  (ta-font-color . "white")      ;; フォント色(無視されます)
  (ta-line-pitch .  0)           ;; 行間ピクセル数
  (ta-char-gap   .  0)           ;; 文字間ピクセル数(無視されます)

  (ta-font-edge-color.r . 0)     ;; エッジ色R
  (ta-font-edge-color.g . 0)     ;; エッジ色G
  (ta-font-edge-color.b . 0)     ;; エッジ色B
  (ta-font-edge-color.a . 255)   ;; エッジ色A(255/エッジあり, 0/エッジなし)

  (ta-ruby-font      .  0)         ;; リソース定義にあるフォント番号を指定します
  (ta-ruby-font-size . 12)         ;; 全角幅をピクセル指定(偶数のみ)
  (ta-ruby-text-color.r . 255)     ;; ルビ本文色R
  (ta-ruby-text-color.g . 255)     ;; ルビ本文色G
  (ta-ruby-text-color.b . 255)     ;; ルビ本文色B
  (ta-ruby-text-color.a . 255)     ;; ルビ本文色A(255固定)
  (ta-ruby-edge-color.r .  90)     ;; ルビエッジ色R
