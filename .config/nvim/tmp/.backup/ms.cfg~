;;
;; 選択肢ウィンドウ
;;
;; (番号 (パラメータ名 . 値) ..)
;; (番号 (パラメータ名 . 値) ..)
;; (番号 (パラメータ名 . 値) ..)
;;
(:select-window

  (0  ;; "default-32"
    (window-x . 50)
    (window-y . 100)
    (window-width . 700)
    ;; !自動調節されるべき!
    ;;(window-height . (+ 20 (* 36 num-show)))
    ;;(window-height . 200)
    (window-height-base . 20)
    (window-color-r . 0)
    (window-color-g . 120)
    (window-color-b . 120)
    (window-color-a . 178)
    (cursor-color-r . 255)
    (cursor-color-g . 255)
    (cursor-color-b . 255)
    (cursor-color-a . 178)
    (item-width .  680)
    (item-height .  32)
    (item-height-gap . 4)
    (item-offset-x . 10)
    (item-offset-y . 10)
    (item-text-color.r . 255)     ;; テキスト色R(デフォルトの？)
    (item-text-color.g . 255)     ;; テキスト色G
    (item-text-color.b . 255)     ;; テキスト色B
    (item-text-edge-color.r .   0)     ;; エッジ色R
    (item-text-edge-color.g .   0)     ;; エッジ色G
    (item-text-edge-color.b .   0)     ;; エッジ色B
    (item-text-edge-color.a . 255)     ;; エッジ色A(0ならエッジをつけない)
    )

