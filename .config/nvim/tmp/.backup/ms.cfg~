  (ta-linenum        .  4)       ;; 表示する行数。(XXX読まない)
  (ta-chars-per-line . 62)       ;; １行あたりの文字数を半角で指定します。

  ;; キーウェイトアイコン設定
  (ta-keywait-icon   .  200)  ;; リソース指定のパーツ番号を指定します。(通常用)
  (ta-keywait-icon-1 .  201)  ;; 代替アイコンその１を指定します。(auto用)

  (ta-keywait-icon-posx . 680);; #Wコマンドでのカーソル表示位置(テキストエリア相対)
  (ta-keywait-icon-posy . 128);; #Eは文末です。

  ;; 表示スピードの定義
  ;; １文字あたりの待機時間をミリ秒で指定します
