;;(255  128  128)
;;(128  255  128)
;;(128  128  255)
;;(255  255  128)
;;(128  255  255)
;;(255  128  255)
;;(255  255  128)
;;(185  185  185)
;;(255  255  255)
;;(255  185  185)
;;(185  255  255)
;;(255  185  185)
;;(185  185  255)
;;(128  128  255)
;;(255  185  185)
;;(255  255  185)
;;(185  185  185)
;;(238  225  185)
;;(225  238  185)
;;(185  225  238)
;;(255  255  255)
;;反映されません。今の中身は下のヤツ。

(:color-table

  ;; 
  ;; フォントカラー設定 ($0〜$9, $A〜$K), 21個
  ;; 
  ;; R,G,Bの順に書きます
  ;; 

;;   ( 0 . ( 0 0 0 ))
;;   ( 1 . ( 0 0 0 ))
;;   ( 2 . ( 0 0 0 ))
;;   ( 3 . ( 0 0 0 ))
;;   ( 4 . ( 0 0 0 ))
;;   ( 5 . ( 0 0 0 ))
;;   ( 6 . ( 0 0 0 ))
;;   ( 7 . ( 0 0 0 ))
;;   ( 8 . ( 0 0 0 ))
;;   ( 9 . ( 255 255 255 ))
;;   (10 . (  24  64 176 ))  ;; $A
;;   (11 . ( 176   0  80 ))  ;; $B
;;   (12 . (  64  24 176 ))  ;; $C
;;   (13 . (   0 128 128 ))  ;; $D
;;   (14 . (  64 128  32 ))  ;; $E
;;   (15 . ( 128  64   0 ))  ;; $F
;;   (16 . ( 176  64 128 ))  ;; $G
;;   (17 . (  68  84   0 ))  ;; $H
;;   (18 . (  96  96   0 ))  ;; $I
;;   (19 . (  64  64  64 ))  ;; $J
;;   (20 . (   0   0   0 ))  ;; $K, default(#N) 暗い配色用

   ( 0 . ( 0 0 0 ))
   ( 1 . ( 0 0 0 ))
   ( 2 . ( 0 0 0 ))
   ( 3 . ( 0 0 0 ))
   ( 4 . ( 0 0 0 ))
   ( 5 . ( 0 0 0 ))
   ( 6 . ( 0 0 0 ))
   ( 7 . ( 0 0 0 ))
   ( 8 . ( 0 0 0 ))
   ( 9 . (   0   0   0 ))
   (10 . ( 128 152 255 ))  ;; $A
   (11 . ( 255 152 192 ))  ;; $B
   (12 . ( 192 152 255 ))  ;; $C
   (13 . ( 192 255 255 ))  ;; $D
   (14 . ( 192 255 152 ))  ;; $E
   (15 . ( 192 216 192 ))  ;; $F
   (16 . ( 255 216 192 ))  ;; $G
   (17 . ( 255 208 208 ))  ;; $H
   (18 . ( 192 192 192 ))  ;; $I
   (19 . ( 255 255 208 ))  ;; $J
   (20 . ( 255 255 255 ))  ;; $K, default(#N)    明るい配色用


)

