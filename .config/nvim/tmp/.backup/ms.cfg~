  ;;(:flip-method "auto")      ;; CPUパワーを食いつぶさない程度に下げる
  (:flip-method "specified") ;; 指定FPSを保とうとする
  ;;(:flip-method-rate 30)      ;; 30fps
  (:flip-method-rate 60)      ;; 60fps
  ;;(:flip-method-rate 40)      ;; 60fps
  ;;(:flip-method-rate 10)      ;; 60fps

