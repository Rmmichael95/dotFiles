; (:script
;
;   ;; 最初に起動するスクリプト
;   ;;
;   ;; 初期化後、この番号のスクリプトをシステムモードで実行する。
;   ;;
;   (:initial-script 9000)
;  
;   ;; その他？
;
;   ;; セーブ
;   ;; ロード
;   ;; コンフィグ
;   ;; 終了確認
;   ;; バックログ
;   ;; フローモード
;   ;; 背景表示
;
;   ;; XXX:現状これらは埋め込み!
;
; )
 

;;----------------------------------------------------------------------
;;
;; リソース定義
;;
;;----------------------------------------------------------------------

(:resource

  ;; ディレクトリは、ゲームのインストール先からの相対指定となります。

  ;;
  ;; ファイル名は以下の方法で合成します。(※音声を除く)
  ;;
  ;;   [basedir]/[basename][digits].[extension]
  ;;


  ;; ----------------------------------------------------------------------
  ;; スクリプト
  ;; ----------------------------------------------------------------------
  (:script-basedir     "scene")
  (:script-basename    "bruns")
  (:script-num-digits  4)
  (:script-extension   "bso")

  ;; ----------------------------------------------------------------------
  ;; 背景画像(pictureloadでBGレイヤにロードされる)
  ;; ----------------------------------------------------------------------
  (:image-basedir    "image")
  (:image-basename   "grp")
  (:image-num-digits 4)
  (:image-extension  "png")

  ;; ----------------------------------------------------------------------
  ;; ストリームアニメ
  ;; ----------------------------------------------------------------------
  (:streamanime-basedir    "parts/streamanime")
  (:streamanime-basename   "bruna")
  (:streamanime-num-digits 4)
  (:streamanime-extension  "")
  
  ;; ----------------------------------------------------------------------
  ;; セル画像(cell_loadでセルレイヤにロードされる)
  ;; ----------------------------------------------------------------------
  (:cellimage-basedir    "image")
  (:cellimage-basename   "grp")
  (:cellimage-num-digits 4)
  (:cellimage-extension  "png")
  
  ;; ----------------------------------------------------------------------
  ;; 音楽
  ;; ----------------------------------------------------------------------
  (:music-basedir     "music")
  (:music-basename    "bruns")
  (:music-num-digits  2)
  (:music-extension   "um3")
  
  ;; ----------------------------------------------------------------------
  ;; 効果音
  ;; ----------------------------------------------------------------------
  (:sound-basedir  "effect")
  (:sound-basename "efc")
  (:sound-num-digits 4)
  (:sound-extension "um3")
  
  ;; ----------------------------------------------------------------------
  ;; 音声  [basedir]/[scene-num]/[prefix][voice-num].[extension]
  ;; ----------------------------------------------------------------------
  (:voice-basedir  "voice")
  (:voice-scene-num-digits 2)
  (:voice-voice-num-digits 5)
  (:voice-voice-prefix "k")
  (:voice-extension "um3")
  
  ;; ----------------------------------------------------------------------
  ;; パーツ（チップ）定義
  ;; ----------------------------------------------------------------------
  (:parts-basedir    "parts")  ;; ベースディレクトリ
  (:parts-basename   "grpc")    ;; ベース名
  (:parts-num-digits 4)      ;; 数値指定の場合の桁数 例:bbp0125.png
  (:parts-extension "png")  ;; 拡張子

  ;; ----------------------------------------------------------------------
  ;; ムービー定義
  ;; ----------------------------------------------------------------------
  (:movie-basedir    "movie")     ;; ベースディレクトリ
  (:movie-basename   "grpmv")  ;; ベース名
  (:movie-num-digits 3)           ;; 数値指定の場合の桁数 例:aobmovie03.mpg
  (:movie-extension  "mpg")       ;; 拡張子

  ;; ----------------------------------------------------------------------
  ;; フォント定義
  ;; ----------------------------------------------------------------------
  ;; 0番がデフォルトになります, 1,2,3 まで入れられます
  (:font-basedir  "font")         ;; ベースディレクトリ
  ;;(:font-regkey "Parts")

  ;(:font-0-type 0)         ;; 0:gdi 1:bitmap(old) 2:bitmap(new)
  ;(:font-0-type 1)         ;; 0:gdi 1:bitmap(old) 2:bitmap(new)
  (:font-0-type 2)         ;; 0:gdi 1:bitmap(old) 2:bitmap(new)
  ;;(:font-0-name "ＭＳ ゴシック")  ;; default
  ;;(:font-0-name "ＭＳ 明朝")

  ;; 旧方式(32,64固定)
  ;(:font-0-0s "ff_00.bmp")   ;; 32dot, 8000
  ;(:font-0-1s "ff_01.bmp")   ;; 32dot, 9000
  ;(:font-0-2s "ff_02.bmp")   ;; 32dot, E000
  ;(:font-0-3s "ff_03.bmp")   ;; 32dot, ASCII
  ;(:font-0-0l "ff_00l.bmp")  ;; 64dot, 8000
  ;(:font-0-1l "ff_01l.bmp")  ;; 64dot, 9000
  ;(:font-0-2l "ff_02l.bmp")  ;; 64dot, E000
  ;(:font-0-3l "ff_03l.bmp")  ;; 64dot, ASCII

  ;; 新方式(サイズ自由)
  ;;   font-(fontno)-(region)-(fontsize)
  (:font-0-0-32 "ff_00.bmp")   ;; 32dot, 8000
  (:font-0-1-32 "ff_01.bmp")   ;; 32dot, 9000
  (:font-0-2-32 "ff_02.bmp")   ;; 32dot, E000
  (:font-0-3-32 "ff_03.bmp")   ;; 32dot, ASCII
  (:font-0-0-64 "ff_00l.bmp")  ;; 64dot, 8000
  (:font-0-1-64 "ff_01l.bmp")  ;; 64dot, 9000
  (:font-0-2-64 "ff_02l.bmp")  ;; 64dot, E000
  (:font-0-3-64 "ff_03l.bmp")  ;; 64dot, ASCII
  ;(:font-0-0-12 "ff_00_12.bmp")  ;; 12dot, 8000
  ;(:font-0-1-12 "ff_01_12.bmp")  ;; 12dot, 9000
  ;(:font-0-2-12 "ff_02_12.bmp")  ;; 12dot, E000
  ;(:font-0-3-12 "ff_03_12.bmp")  ;; 12dot, ASCII

  (:font-1-type 0)         ;; 0:gdi 1:bitmap
  (:font-1-name "ＭＳ 明朝")
  ;(:font-1-name "HG丸ｺﾞｼｯｸM-PRO")
  ;;(:font-1-0s "ff_00.bmp")   ;; 32dot, 8000
  ;;(:font-1-1s "ff_01.bmp")   ;; 32dot, 9000
  ;;(:font-1-2s "ff_02.bmp")   ;; 32dot, E000
  ;;(:font-1-3s "ff_03.bmp")   ;; 32dot, ASCII
  ;;(:font-1-0l "ff_00l.bmp")  ;; 64dot, 8000
  ;;(:font-1-1l "ff_01l.bmp")  ;; 64dot, 9000
  ;;(:font-1-2l "ff_02l.bmp")  ;; 64dot, E000
  ;;(:font-1-3l "ff_03l.bmp")  ;; 64dot, ASCII

  (:font-2-type 0)         ;; 0:gdi 1:bitmap
  (:font-2-name "ＭＳ ゴシック")

  ;; ----------------------------------------------------------------------
  ;; セーブデータ
  ;; ----------------------------------------------------------------------
  (:save-basedir     "save")
  (:save-basename    "brunssv")
  (:save-num-digits  2)
  (:save-extension   "dat")
  ;; システム情報の保存先 (フォルダは save-basedir)
  (:save-syssave-name "bruns-system.dat")
  ;; デフォルト値(初期データ)
  (:save-syssave-default-name "bruns-system0.dat")

  ;; 画像ストレージ
  ;;(:storage-basedir    "save")
  ;;(:storage-basename   "brunss")
  ;;(:storage-num-digits 4)
  ;;(:storage-extension  "png")
  ;; 画像ストレージ
  (:storage-basedir    "image")
  (:storage-basename   "grp")
  (:storage-num-digits 4)
  (:storage-extension  "png")

  ;; ----------------------------------------------------------------------
  ;; カーソル定義
  ;; 現状、Windowsのカーソルファイル(.cur,.ani)しか使えません
  ;; ----------------------------------------------------------------------
  (:cursor-basedir    "parts")    ;; ベースディレクトリ
  (:cursor-basename   "bruns")    ;; ベース名
  (:cursor-num-digits 2)           ;; 数値指定の場合の桁数 例:cursor03.cur
  (:cursor-extension  "ani")       ;; 拡張子
)


;; ----------------------------------------------------------------------
;;
;; 数値定数・文字列定数
;;
;; (定数番号 . 数値)
;; (定数番号 . "文字列")
;;
;; 数値定数は   global_flag_read   でreflagに取り出せます
;; 文字列定数は global_string_read で文字列バッファに読み出すことができます。
;;
;; ----------------------------------------------------------------------

(:flag-constants
  ;;(0 . 12)

  ;; 初期起動するシステムスクリプト番号  
  (1 . 9010)

  ;; モノローグ送り速度(待機ミリ秒数)
  ;; 低速（１０秒）
  (10 . 5000)
  ;; 中速（７秒）
  (11 . 3000)
  ;; 高速（４秒）
  (12 . 1000)

  ;; バックログ文字色（互換モード）
  (20 . 0xffc8c8ff)     ;; ARGB, デフォルト 0xffffffff
  ;; バックログ選択肢の文字色とフチ色
  (22 . 0xffc8c8ff)     ;; ARGB, デフォルト 0xffc8c8ff
  (23 . 0xff000000)     ;; ARGB, デフォルト 0xff000000
  ;; バックログ選択結果の文字色とフチ色
  (24 . 0xffffffc8)     ;; ARGB, デフォルト 0xffffffc8
  (25 . 0xff000000)     ;; ARGB, デフォルト 0xff000000
  ;; バックログセパレータの高さ
  (26 . 20)             ;; pixel, デフォルト２０

  ;; デフォルトのウィンドウ設定番号
  (30 . 0)

  ;; フローモード用パーツ番号
  ;; イベントアイコンのパーツ番号。この値と＋１を使う。
  (100 . 9020)
  ;; 分岐アイコンのパーツ番号。この値と＋１を使う。
  (101 . 9023)
  ;; 開始アイコンのパーツ番号。この値と＋１を使う。
  (102 . 9026)
  ;; 終端アイコンのパーツ番号。この値と＋１を使う。
  (103 . 9029)
  ;; 矢印用アイコンのパーツ番号。この値から＋８までを使う。
  ;;   +0  カド右上(86)
  ;;   +1  カド右下(26)
  ;;   +2  カド左下(24)
  ;;   +3  カド左上(84)
  ;;   +4  水平線
  ;;   +5  垂直線
  ;;   +6  右矢印
  ;;   +7  上矢印
  ;;   +8  下矢印
  (104 . 9010)
)

(:string-constants
  ;;(0 . "デフォルト")
  ;;(1 . "未定義")
  ;;(2 . "俺俺")
)

